{
  "name": "express-react-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "config": {
    "CLIENR_DIRECTORY": "dist/shop"
  },
  "scripts": {
    "build:client": "vite build --outDir $npm_package_config_CLIENR_DIRECTORY",
    "build:server": "vite build --ssr React/Server.jsx --outDir $npm_package_config_CLIENR_DIRECTORY",
    "build:copy": "cp -rf public/dist/* $npm_package_config_CLIENR_DIRECTORY",
    "build": "npm run build:client && npm run build:server && npm run build:copy",
    "dev": "node Develop.mjs",
    "serve": "node Server.mjs",
    "tailwind": "npx tailwindcss -i ./React/src/input.css -o ./React/src/assets/css/output.css --watch --minify"
  },
  "dependencies": {
    "auto-bind": "^5.0.1",
    "axios": "^1.7.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.19.2",
    "html-entities": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.24.0",
    "vite-plugin-minify": "^1.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "tailwindcss": "^3.4.3",
    "vite": "^5.3.1",
    "vite-plugin-environment": "^1.1.3"
  }
}